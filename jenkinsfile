pipeline {
    agent any
    stages {
        
         stage('Build') {
            steps {
                echo "building the project"
                bat "mvn clean"
            }
        }
         stage('Test') {
            steps {
                echo "Testing the project"
                bat "mvn test"
            }
        }
        stage('compile') {
            steps {
                echo "compiling the project"
                bat "mvn compile"
            }
        stage('Deploy') {
            steps {
                echo "Deploy the project"
                
            }
        }
        
        
        }
    }
}
